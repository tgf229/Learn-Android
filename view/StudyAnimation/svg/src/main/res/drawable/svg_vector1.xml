<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="200dp"
    android:width="200dp"
    android:viewportWidth="100"
    android:viewportHeight="100"
    >
    <!-- width height表示SVG图形具体大小 -->
    <!-- viewportWidth viewportHeight 表示SVG图形划分的比例,后面绘制path的参数就是根据比例来的 -->
    <group>
        <path
            android:name="path1"
            android:strokeColor="@android:color/holo_blue_bright"
            android:strokeLineCap="round"
            android:strokeWidth="5"
            android:pathData="M 20,20 L 50,20,80,20"
            />
        <path
            android:name="path2"
            android:strokeColor="@android:color/holo_green_light"
            android:strokeLineCap="round"
            android:strokeWidth="5"
            android:pathData="M 20,80 L 50,80,80,80"
            />
    </group>
</vector>